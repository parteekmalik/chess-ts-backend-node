{
  "name": "express-api-starter-ts",
  "version": "1.2.0",
  "description": " A basic starter for an express.js API with Typescript",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/data.ts",
    "querry": "ts-node src/api/querrymannuall.ts",
    "start:server1": "nodemon --exec ts-node src/api/server.ts",
    "start:server2": "nodemon --exec ts-node src/index1.ts",
    "start:server3": "nodemon --exec ts-node src/api/matchMaking.ts",
    "dev": "concurrently \"npm run start:server1\" \"npm run start:server2\" \"npm run start:server3\"",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate dev"
  },
  "keywords": [],
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "chess.js": "^1.0.0-beta.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.6.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.199",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.7.1",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "concurrently": "^8.2.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "helmet": "^7.0.0",
    "jest": "^29.5.0",
    "mongodb": "^6.1.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
